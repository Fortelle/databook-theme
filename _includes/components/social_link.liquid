<tr class="c-sociallist__item">
  <td class="c-sociallist__icon">
    {%- if include.icon -%}
      {%- include functions/get_icon.liquid icon=include.icon -%}
    {%- endif -%}
  </td>
  <td class="c-sociallist__text">
    {%- if include.url -%}
      <a href="{{- include.url | relative_url -}}" target="_blank">{{- include.text -}}</a>
    {%- else -%}
      {{- include.text -}}
    {%- endif -%}
  </td>
</tr>
